package com.lti.MyException;

public class CarPartException extends Exception
{

	public CarPartException() {
		super();
		// TODO Auto-generated constructor stub
	}

	public CarPartException(String message, Throwable cause, boolean enableSuppression, boolean writableStackTrace) {
		super(message, cause, enableSuppression, writableStackTrace);
		// TODO Auto-generated constructor stub
	}

	public CarPartException(String message, Throwable cause) {
		super(message, cause);
		// TODO Auto-generated constructor stub
	}

	public CarPartException(String message) {
		super(message);
		// TODO Auto-generated constructor stub
	}

	public CarPartException(Throwable cause) {
		super(cause);
		// TODO Auto-generated constructor stub
	}
	
	

}