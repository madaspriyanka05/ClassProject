package com.lti.test;

import java.time.LocalDate;

import org.junit.Test;

import com.lti.dao.GenericDao;
import com.lti.model.*;
import com.lti.service.AccountService;

public class TestAccount {

	@Test
	public void testOpenAccount() 
	{
		Account a1=new Account();
		a1.setAccNo(1001);
		a1.setAccName("Priyanka");
		a1.setAccBal(500);
		a1.setAcctype("Saving");
		
		
		Account a2=new Account();
		a2.setAccNo(1002);
		a2.setAccName("Poja");
		a2.setAccBal(600);
		a2.setAcctype("Current");
		

		
		Account a3=new Account();
		a3.setAccNo(1003);
		a3.setAccName("Laxmi");
		a3.setAccBal(700);
		a3.setAcctype("Saving");
		
		
		Account a4=new Account();
		a4.setAccNo(1004);
		a4.setAccName("Asmita");
		a4.setAccBal(800);
		a4.setAcctype("Current");
		
		Account a5=new Account();
		a5.setAccNo(1005);
		a5.setAccName("Shruti");
		a5.setAccBal(900);
		a5.setAcctype("Current");
		

		
		AccountService acc=new AccountService();
		acc.openAccount(a5);
		acc.openAccount(a4);
		acc.openAccount(a3);
		acc.openAccount(a2);
		acc.openAccount(a1);
		
	}
	
	
	@Test
	public void testOpenTranasaction() 
	{
		
		AccountService as=new AccountService();
		as.withdraw(1001,500);
	}
	
	
	@Test
	public void testdepositTranasaction() 
	{
		
		AccountService as=new AccountService();
		as.deposit(1002,200);
	
	}
	
	@Test
	public void testTransferTranasaction() 
	{
		
		AccountService as=new AccountService();
		as.transfer(1002, 1001, 500);
	
	}

}
